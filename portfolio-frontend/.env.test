# Test environment configuration for frontend
# This file contains environment variables used during testing

# API URL for testing
REACT_APP_API_URL=http://localhost:3001/api

# Disable analytics and external services during testing
REACT_APP_DISABLE_ANALYTICS=true
REACT_APP_DISABLE_TRACKING=true

# Test-specific settings
REACT_APP_ENV=test
GENERATE_SOURCEMAP=false

# Skip preflight check for testing
SKIP_PREFLIGHT_CHECK=true
